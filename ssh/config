Host jumper.xxx.com
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    # 空闲4小时候自动断开，能够避免下次连接时长时间卡主
    ControlPersist 4h
Host github.com
    HostName github.com
    User gaochundong
    IdentitiesOnly yes
    AddKeysToAgent yes
    IdentityFile /Users/gaochundong/g/service/ssh/github/id_ed25519
Host 192.168.64.2
    HostName 192.168.64.2
    User ubuntu
    IdentitiesOnly yes
    AddKeysToAgent yes
    IdentityFile /Users/gaochundong/g/service/ssh/multipass/id_ed25519
